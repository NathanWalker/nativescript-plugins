{
	"version": 1,
	"projects": {
		"demo": {
			"root": "apps/demo/",
			"sourceRoot": "apps/demo/src",
			"projectType": "application",
			"prefix": "demo",
			"architect": {
				"ios": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug ios --no-hmr --env.testing"],
						"cwd": "apps/demo",
						"parallel": false
					}
				},
				"android": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug android --no-hmr --env.testing"],
						"cwd": "apps/demo",
						"parallel": false
					}
				},
				"clean": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns clean", "npm i"],
						"cwd": "apps/demo",
						"parallel": false
					}
				}
			}
		},
		"demo-angular": {
			"root": "apps/demo-angular/",
			"sourceRoot": "apps/demo-angular/src",
			"projectType": "application",
			"prefix": "demo",
			"architect": {
				"ios": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug ios --no-hmr --env.testing"],
						"cwd": "apps/demo-angular",
						"parallel": false
					}
				},
				"android": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug android --no-hmr --env.testing"],
						"cwd": "apps/demo-angular",
						"parallel": false
					}
				},
				"clean": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns clean", "npm i"],
						"cwd": "apps/demo-angular",
						"parallel": false
					}
				}
			}
		},
		"all": {
			"root": "packages",
			"projectType": "library",
			"architect": {
				"build": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx run emoji-picker:build.all", "nx run squirrel-chat-ui:build.all"],
						"parallel": false
					}
				},
				"focus": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages"],
						"parallel": false
					}
				}
			}
		},
		"emoji-picker": {
			"root": "packages/emoji-picker",
			"sourceRoot": "packages/emoji-picker",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/node:package",
					"options": {
						"outputPath": "dist/packages/emoji-picker",
						"tsConfig": "packages/emoji-picker/tsconfig.json",
						"packageJson": "packages/emoji-picker/package.json",
						"main": "packages/emoji-picker/index.ts",
						"assets": [
							"packages/emoji-picker/*.md",
							"packages/emoji-picker/index.d.ts",
							"LICENSE",
							{
								"glob": "**/*",
								"input": "packages/emoji-picker/platforms/",
								"output": "./platforms/"
							}
						]
					}
				},
				"build.all": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx run emoji-picker:build", "node tools/scripts/build-finish.ts emoji-picker"],
						"parallel": false
					}
				},
				"focus": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages emoji-picker"],
						"parallel": false
					}
				}
			}
		},
		"squirrel-chat-ui": {
			"root": "packages/squirrel-chat-ui",
			"sourceRoot": "packages/squirrel-chat-ui",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/node:package",
					"options": {
						"outputPath": "dist/packages/squirrel-chat-ui",
						"tsConfig": "packages/squirrel-chat-ui/tsconfig.json",
						"packageJson": "packages/squirrel-chat-ui/package.json",
						"main": "packages/squirrel-chat-ui/index.ts",
						"assets": [
							"packages/squirrel-chat-ui/*.md",
							"packages/squirrel-chat-ui/index.d.ts",
							"LICENSE",
							{
								"glob": "**/*",
								"input": "packages/squirrel-chat-ui/platforms/",
								"output": "./platforms/"
							}
						]
					}
				},
				"build.all": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx run squirrel-chat-ui:build", "node tools/scripts/build-finish.ts squirrel-chat-ui"],
						"parallel": false
					}
				},
				"focus": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages squirrel-chat-ui"],
						"parallel": false
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/workspace"
	}
}
